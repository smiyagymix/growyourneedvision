  "scripts": {
    "dev": "concurrently \"pnpm:dev:pocketbase\" \"pnpm:dev:init\" \"pnpm:dev:client\" \"pnpm:dev:ai\" \"pnpm:dev:server\" --names \"POCKETBASE,INIT,CLIENT,AI,SERVER\" --prefix-colors \"magenta,cyan,blue,yellow,green\"",
    "dev:init": "node scripts/startup-init.js",
    "dev:client": "vite",
    "dev:pocketbase": "cd pocketbase && pocketbase.exe serve --http=127.0.0.1:8090",
    "dev:ai": "powershell -ExecutionPolicy Bypass ./launch-ai.ps1",
    "dev:server": "cd server && npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --max-warnings 0",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lhci": "lhci autorun",
    "db:setup-files": "node scripts/maintenance/create-file-uploads-collection.js",
